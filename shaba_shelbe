def menu(my_name,my_id):
	try:
		token = open('.token.txt','r').read()
		cok = open('.cok.txt','r').read()
	except IOError:
		print('[×] Cookies NO ')
		time.sleep(5)
		MRDYNO()
	os.system('clear')
	banner()
	print('')
	print('')
	print('\x1b[1;91m[ 1 ] CRACK PUBLIC \033[33m[ \033[32mON \033[33m]')
	print('')
	print('\x1b[1;91m[ 2 ] CRACK FILE \033[33m[ \033[32mON \033[33m]')
	print('')
	print('\x1b[1;91m[ 0 ] REMOVIE COOKIE ')
	print('')
	_alvinoadijaya___ = input('\n\x1b[1;91mCHOICE ≫  ')
	print('')
	if _alvinoadijaya___ in ['1']:
		dump_massal()
	elif _alvinoadijaya___ in ['2']:
		File2()
	elif _alvinoadijaya___ in ['30']:
		result()
	elif _alvinoadijaya___ in ['40']:
	    cek_opsi()
	elif _alvinoadijaya___ in ['0']:
		os.system('rm -rf .token.txt')
		os.system('rm -rf .cok.txt')
		print('\x1b[1;93m[\x1b[1;92m!\x1b[1;93m]\x1b[1;93m ╰─>\x1b[1;92mSukses Logout\x1b[1;93m+\x1b[1;92mHapus Kukis ')
		exit()
	else:
		print('\x1b[1;93m[\x1b[1;92m!\x1b[1;93m]\x1b[1;93m ╰─>\x1b[1;92mPilih Yang Bener ')
